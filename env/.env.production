# 变量必须以 VITE_ 为前缀才能暴露给外部读取
NODE_ENV = 'production'
# 是否去除console 和 debugger
VITE_DELETE_CONSOLE = true
# 是否开启sourcemap
VITE_SHOW_SOURCEMAP = false

# 后台请求地址
# VITE_SERVER_BASEURL = 'http://localhost:8001'

# WebSocket 连接地址（包含端点）
# VITE_WS_URL = 'wss://api.example.com/ws/connect'
# 是否启用 WebSocket
# VITE_WS_ENABLE = true
